print(bool(x))
x = True
print(bool(x))
x = 5
y = 10
print(bool(x == y))
x = None
print(bool(x))
x = ()
print(bool(x))
x = {}
print(bool(x))
x = 0.0
print(bool(x))
x = 'durgasri'
print(bool(x))
arr1 = bytearray(b"abcd")
for value in arr1:
	print(value)
arr2 = bytearray(b"aaaacccc")
print("Count of c is:", arr2.count(b"c"))
