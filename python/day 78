s1 = "geek"
obj1 = enumerate(l1)
obj2 = enumerate(s1)
print ("Return type:", type(obj1))
print (list(enumerate(l1)))
print (list(enumerate(s1, 2)))
for i in range(10):
	print(i, end=" ")
print()
l = [10, 20, 30, 40]
for i in range(len(l)):
	print(l[i], end=" ")
print()
sum = 0
for i in range(1, 11):
	sum = sum + i
print("Sum of first 10 natural number :", sum)
