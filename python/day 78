float = -54.26
print('Absolute value of float is:', abs(float))
int = -94
print('Absolute value of integer is:', abs(int))
complex = (3 - 4j)
print('Absolute value or Magnitude of complex is:', abs(complex))
l = [4, 5, 1]
print(all(l))
l = [0, 0, False]
print(all(l))
l = [1, 0, 6, 7, False]
print(all(l))
l = []
print(all(l))
l = [ 4, 5, 1]
print(any( l ))
l = [ 0, 0, False]
print(any( l ))
l = [ 1, 0, 6, 7, False]
print(any( l ))
l = []
print(any( l ))
