#reverse a list
def reverse(lst):
    lst.reverse()
    return lst

lst = ['viswasam','master','airaa']
print(reverse(lst))

def swplst(list):
    get=list[-1],list[0]
    list[0],list[-1]=get
    return list
list=[3,57,87,97,67,1002]
print( swplst(list))

