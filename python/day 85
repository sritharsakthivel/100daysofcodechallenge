print(len("sri"))
print(len([10, 20,30]))
def add(x, y, z = 0):
	return x + y+z
print(add(2, 3))
print(add(2, 3, 4))
class India():
	def capital(self):
		print("New Delhi is the capital of India.")
	def language(self):
		print("Hindi is the most widely spoken language of India.")
	def type(self):
		print("India is a developing country.")
class USA():
	def capital(self):
		print("Washington, D.C. is the capital of USA.")
	def language(self):
		print("English is the primary language of USA.")
	def type(self):
		print("USA is a developed country.")
obj_ind = India()
obj_usa = USA()
for country in (obj_ind, obj_usa):
	country.capital()
	country.language()
	country.type()
class agaram:
    def intro(self):
        print("agaramn is good platform in sudent.")
    def flight(self):
	     print("agaram student is most of talented oerson.")
class sparrow(agaram):
    def flight(self):
	     print("3k student is graduate in agaram.")
class ostrich(agaram):
    def flight(self):
	     print("4k student studying in agaram.")
obj_bird = agaram()
obj_spr = sparrow()
obj_ost = ostrich()

obj_bird.intro()
obj_bird.flight()

obj_spr.intro()
obj_spr.flight()

obj_ost.intro()
obj_ost.flight()
